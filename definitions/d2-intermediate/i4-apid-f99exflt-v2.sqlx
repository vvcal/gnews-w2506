config {
  type: "view",
  schema: "c1d1_o1flt",
  name: "s1_apid_flt_f99exflt_vt",
  description: "Filtered APID view excluding linkfest and earnings mentions"
}

SELECT
  date,
  title,
  summary,
  links
  -- codaSourceRowURLF (commented out)
FROM
  ${ref("s3-apid-mv")}
WHERE
  LOWER(title) NOT LIKE '%linkfest%'
  AND LOWER(summary) NOT LIKE '%earning%'

  

